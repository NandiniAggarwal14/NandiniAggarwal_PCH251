#include<stdio.h>
int main()
{
int a[10][10],m,n,i,j;
printf("\nEnter the  number of rows and columns: ");
scanf("%d%d",&m,&n);
printf("\nEnter array elements: ");
for(i=0;i<m;i++)
{
for(j=0;j<n;j++)
{
scanf("%d",&a[i][j]);
}
}
printf("\nThe array elements are as follows:\n");
for(i=0;i<m;i++)
{
for(j=0;j<n;j++)
{
printf("%d ",a[i][j]);
}
printf("\n");
}
return 0;
}
