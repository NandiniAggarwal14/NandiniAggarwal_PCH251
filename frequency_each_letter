#include<string.h>
#include<stdio.h>
int main()
{
    int a,j,l;
    char s[20],i;
    printf("Enter string\n");
    gets(s);
    l=strlen(s);
    for(i='a';i<='z';i++)
    {
        a=0;
        j=0;
        while(j<=l)
        {
        if(s[j]==i)
        a++;
        j++;
        }
        if(a!=0)
        printf("frequency of %c = %d\n",i,a);
    }
    return 0;
}
